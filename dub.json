{
	"name": "numd",
	"description": "A numerical computing package for D",
	"copyright": "Copyright Â© 2016, Robert F. Rau II",
	"authors": ["Rob Rau"],
	"targetType": "sourceLibrary",
	"sourcePaths": [
		"numd/linearalgebra"
	],
	"importPaths": ["numd"],
	"dependencies": {
		"cblas": "~>0.1.0",
		"scid": "~>0.3.0"
	},
	"buildTypes": {
		"unittest": {
			"buildOptions": ["unittests", "debugMode", "debugInfo"]
		},
		"ldc-release": {
			"buildOptions": ["releaseMode", "inline"],
			"dflags": ["-O5"]
		},
		"ldc-release-prof": {
			"buildOptions": ["releaseMode", "inline", "debugInfo"],
			"dflags": ["-O5"]
		}
	}
}
